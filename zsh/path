# vim:syntax=sh

# Homebrew PATH
HOMEBREWPATH=$HOME/.homebrew/bin:$HOME/.homebrew/sbin
PATH=$HOMEBREWPATH:$PATH

# Heroku Toolbelt PATH
HEROKU=/usr/local/heroku/bin
PATH=$PATH:$HEROKU

# NPM PATH
NPM=/usr/local/share/npm/bin
PATH=$PATH:$NPM

# GO PATH
GOPATH=~/dev/personal/go
export GOPATH
PATH=$PATH:$GOPATH/bin

# NODE PATH
NODE_PATH=/usr/local/lib/node:/Users/damselem/.homebrew/lib/node_modules
export NODE_PATH
PATH=$PATH:$NODE_PATH

# POSTGRESAPP PATH
POSTGRESAPP_PATH=/Applications/Postgres.app/Contents/Versions/9.3/bin
PATH=$PATH:$POSTGRESAPP_PATH

export PATH
